<%= simple_form_for [@employee, @educational_attainment], url: :employee_educational_attainments do |f| %>
<%=f.error_notification %>
<br>
<br>
<br>
<br>
  <div class="col-md-4 col-md-offset-4">
    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="widget-user-username">Educational Attainment Details</h3>
      </div>
      <div class="box-body">
        <%=f.input :degree %>
      </div>
      <div class="box-footer">
        <%=f.submit "Save", class: "btn btn-primary pull-right" %>
      </div>
    </div>
  </div>
<% end %>

