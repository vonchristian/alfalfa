$("ul.errors").html("")
<% if @item_category.errors.any? %>
  <% @item_category.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $("#item-category-modal").modal("hide")
  $("#supplies_inventory_item_category_id").load(location.href + " #supplies_inventory_item_category_id>*", "");
<% end %>
