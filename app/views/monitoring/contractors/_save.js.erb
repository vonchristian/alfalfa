$("ul.errors").html("")
<% if @contractor.errors.any? %>
  <% @contractor.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
	$(".contractor-index").html("<%= escape_javascript(render 'index') %>")
  $("#contractor-modal").modal("hide")
<% end %>
