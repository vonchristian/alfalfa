<%= simple_form_for ([@project, @employment]) do |f| %>
  <%=f.error_notification %>
  <table class="table">
    <tbody>
    <tr>
      <td style="width: 10%"><%=f.input :employee_id, collection: Employee.all, label: false, placeholder: 'Employee' %></td>
</tr>
    </tbody>
    </table>
    <div class="modal-footer">
      <%=f.submit "Add this Employee", class: "btn btn-primary" %>
      <%=link_to "or I'm done adding Employees",project_path(@project) %>
    </div>
<% end %>
